@{
    ViewBag.Title = "ChangeProfile";
}

<h2>ChangeProfile</h2>

@if (Request.IsAuthenticated)
{
    <div>

    @using (Html.BeginForm())
    {
        @Html.EditorForModel()
        <button type="submit">OK</button>
    }
    <div>
        @Html.ActionLink("Back to Profile", "Profile")
    </div>
    <div>
        @Html.ActionLink("Change Email", "ChangeEmail")
    </div>

    </div>
}

